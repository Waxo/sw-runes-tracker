{{#paper-toolbar as |toolbar|}}
  {{#toolbar.tools}}
    <h2>SW Runes Tracker</h2>
    {{#paper-button href=(href-to "import")}}
      {{paper-icon "swap-vert"}}
      Import
    {{/paper-button}}
    {{#paper-button href=(href-to "lock-rune")}}
      {{paper-icon "lock"}}
      Lock runes
    {{/paper-button}}
    {{#paper-button href=(href-to "view-rune")}}
      {{paper-icon "search"}}
      View
    {{/paper-button}}
    <span class="flex"></span>
    v1.0.0-beta
  {{/toolbar.tools}}
{{/paper-toolbar}}

{{yield}}
